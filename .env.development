# Development Environment Variables
# This file provides default values for local development
# Copy to .env to use these values, or create your own .env with real Firebase config

# Development Firebase Configuration (using emulators)
FIREBASE_PROJECT_ID=demo-synther-project
FIREBASE_API_KEY=demo-api-key-for-development
FIREBASE_AUTH_DOMAIN=demo-synther-project.firebaseapp.com
FIREBASE_STORAGE_BUCKET=demo-synther-project.appspot.com

# Demo App IDs (for development)
FIREBASE_APP_ID_WEB=1:123456789:web:demo123456
FIREBASE_APP_ID_ANDROID=1:123456789:android:demo123456
FIREBASE_APP_ID_IOS=1:123456789:ios:demo123456

# Demo Messaging
FIREBASE_MESSAGING_SENDER_ID=123456789

# Demo Analytics
FIREBASE_MEASUREMENT_ID=G-DEMO1234

# Demo iOS Bundle
FIREBASE_IOS_BUNDLE_ID=com.example.synther.dev

# Development mode flag
FLUTTER_ENV=development
USE_FIREBASE_EMULATORS=true